[display]

window/size/viewport_width=1440
window/size/viewport_height=810
window/stretch/mode="viewport"
window/stretch/aspect="ignore"

[global_group]

ground=""
furble=""
bottles=""
level_base_layer=""

[layer_names]

2d_physics/layer_1="World"
2d_physics/layer_2="Bottles"
2d_physics/layer_3="Furbles"
2d_physics/layer_4="Obstacle Effects"
2d_physics/layer_5="Wooden Barricade Obstacle"
2d_physics/layer_6="Water Furble"
2d_physics/layer_7="Shelf"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true
2d/snap/snap_2d_transforms_to_pixel=true
